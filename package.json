{
  "name": "mining-v2",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "test": "yarn run ts-mocha -p ./tsconfig.json -t 200000 tests/**/*.ts",
    "build:program": "anchor build",
    "mainnet:deploy": "bash scripts/mainnet-v2-deploy.sh",
    "mainnet:smoke": "ts-node scripts/mainnet-v2-smoke.ts",
    "claim-mind-bot": "ts-node scripts/claim-mind-bot.ts",
    "withdraw-staking-rewards": "ts-node scripts/withdraw-staking-rewards.ts",
    "melt:testnet:flow": "ANCHOR_PROVIDER_URL=https://rpc.testnet.x1.xyz ts-node scripts/melt-v1-flow-testnet.ts",
    "melt:testnet:admin": "ANCHOR_PROVIDER_URL=https://rpc.testnet.x1.xyz ts-node scripts/melt-v1-flow-admin-only.ts",
    "melt:testnet:2users": "ANCHOR_PROVIDER_URL=https://rpc.testnet.x1.xyz ts-node scripts/melt-v1-flow-2users-testnet.ts",
    "melt:testnet:scenarios": "ANCHOR_PROVIDER_URL=https://rpc.testnet.x1.xyz ts-node scripts/melt-v1-scenarios-testnet.ts",
    "melt:testnet:migrate": "ANCHOR_PROVIDER_URL=https://rpc.testnet.x1.xyz ts-node scripts/melt-v1-migrate-testnet.ts",
    "melt:mainnet:init": "ANCHOR_PROVIDER_URL=https://rpc.mainnet.x1.xyz ts-node scripts/melt-v1-init-mainnet.ts",
    "melt:mainnet:migrate": "ANCHOR_PROVIDER_URL=https://rpc.mainnet.x1.xyz ts-node scripts/melt-v1-migrate-mainnet.ts",
    "melt:testnet:autofinalize": "ANCHOR_PROVIDER_URL=https://rpc.testnet.x1.xyz ts-node scripts/melt-v1-autofinalize-testnet.ts --watch --interval-sec 15",
    "mining:testnet:melt-migrate": "ANCHOR_PROVIDER_URL=https://rpc.testnet.x1.xyz ts-node scripts/mining-v2-melt-migrate-testnet.ts",
    "mining:mainnet:melt-migrate": "ANCHOR_PROVIDER_URL=https://rpc.mainnet.x1.xyz ts-node scripts/mining-v2-melt-migrate-mainnet.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@metaplex-foundation/mpl-token-metadata": "2.13.0",
    "@solana/spl-token": "^0.4.7",
    "@solana/web3.js": "^1.89.0",
    "bn.js": "^5.2.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.1",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "chai": "^4.4.1",
    "mocha": "^10.4.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "main": "index.js",
  "repository": "git@github.com:wielop/mining.git",
  "author": "wielop <wielop@users.noreply.github.com>"
}
